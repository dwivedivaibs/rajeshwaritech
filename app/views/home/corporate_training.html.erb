<main id="content" class="neve-main" role="main">
	<div class="container single-page-container">
		<div class="row">
			<div class="nv-single-page-wrap col">
				<div class="nv-content-wrap entry-content">
					<div class="wp-block-cover alignfull is-light" style="min-height:700px">
            <span aria-hidden="true" class="wp-block-cover__background has-background-dim-0 has-background-dim"></span><img decoding="async" width="1366" height="768" class="wp-block-cover__image-background wp-image-3420" alt="" src="<%= asset_path('corporate-training.jpg')%>" style="object-position:57% 54%" data-object-fit="cover" data-object-position="57% 54%" />
            <div class="wp-block-cover__inner-container">
              <div style="height:80px" aria-hidden="true" class="wp-block-spacer"></div>
              <div style="height:80px" aria-hidden="true" class="wp-block-spacer"></div>
              <div style="height:80px" aria-hidden="true" class="wp-block-spacer"></div>
            </div>
          </div>
          <div class="nv-page-title-wrap nv-big-title">
						<div class="nv-page-title" style="margin-top: 30px;">
							<center>
								<h1 style="border-bottom: 5px solid #134668;width: fit-content;">Corporate Training</h1>
								<p class="has-text-align-left has-text-color" style="color:#121212;font-size:17px;margin-top: 30px;">We provides tailored on-site corporate training in cutting-edge technologies like Software Automation,  DevOps, Data Science & Artificial Intelligence, Machine Learning, Robotics Process Automation.</p>
								<p class="has-text-align-left has-text-color" style="color:#121212;font-size:17px;margin-top: 30px;">Every organization is adopting engineering culture and talent transformation to meet ever-growing customer needs. Our seasoned consultants & practitioners work with you to design the best training program for you.</p>
							</center>
						</div>
					</div>
					<div class="is-layout-flex wp-container-6 wp-block-columns">
						<div class="is-layout-flow wp-block-column">
							<div class="wp-block-qubely-infobox qubely-block-e5c808">
								<div class="qubely-block-info-box  qubely-info-box-layout-1">
									<div class="qubely-info-box-media qubely-media-has-bg">
										<img decoding="async" class="qubely-info-box-image" src="<%= asset_path('sdet.png')%>" srcset="" alt=""/>
									</div>
									<div class="qubely-info-box-body">
										<div class="qubely-info-box-title-container  qubely-separator-position-top">
											<div class="qubely-info-box-title-inner">
												<h3 class="qubely-info-box-title"><strong><strong>SDET and Continuous Delivery</strong></strong></h3>
											</div>
										</div>
										<div class="qubely-block-btn-wrapper">
											<div class="qubely-block-btn">
												<a class="qubely-block-btn-anchor is-medium" href="../test-automation-continuous-delivery/index.html">Know More</a>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="is-layout-flow wp-block-column">
							<div class="wp-block-qubely-infobox qubely-block-b40eec"><div class="qubely-block-info-box  qubely-info-box-layout-1"><div class="qubely-info-box-media qubely-media-has-bg"><img decoding="async" class="qubely-info-box-image" src="<%= asset_path('front.png')%>" srcset="" alt=""/></div><div class="qubely-info-box-body"><div class="qubely-info-box-title-container  qubely-separator-position-top"><div class="qubely-info-box-title-inner"><h3 class="qubely-info-box-title"><strong><strong><strong>Frontend Technologies<br><br></strong></strong></strong></h3></div></div><div class="qubely-block-btn-wrapper"><div class="qubely-block-btn"><a class="qubely-block-btn-anchor is-medium" href="../on-site-corporate-training/index.html">Know More</a></div></div></div></div></div>
						</div>
						<div class="is-layout-flow wp-block-column">
							<div class="wp-block-qubely-infobox qubely-block-b40eec"><div class="qubely-block-info-box  qubely-info-box-layout-1"><div class="qubely-info-box-media qubely-media-has-bg"><img decoding="async" class="qubely-info-box-image" src="<%= asset_path('backend.jpg')%>" srcset="" alt=""/></div><div class="qubely-info-box-body"><div class="qubely-info-box-title-container  qubely-separator-position-top"><div class="qubely-info-box-title-inner"><h3 class="qubely-info-box-title"><strong><strong><strong>Backend Technologies<br><br></strong></strong></strong></h3></div></div><div class="qubely-block-btn-wrapper"><div class="qubely-block-btn"><a class="qubely-block-btn-anchor is-medium" href="../on-site-corporate-training/index.html">Know More</a></div></div></div></div></div>
						</div>
					</div>
					<div class="is-layout-flex wp-container-6 wp-block-columns">
						<div class="is-layout-flow wp-block-column">
							<div class="wp-block-qubely-infobox qubely-block-bfeede"><div class="qubely-block-info-box  qubely-info-box-layout-1"><div class="qubely-info-box-media qubely-media-has-bg"><img decoding="async" class="qubely-info-box-image" src="<%= asset_path('devops-continuous-delivery.jpg')%>" srcset="" alt=""/></div><div class="qubely-info-box-body"><div class="qubely-info-box-title-container  qubely-separator-position-top"><div class="qubely-info-box-title-inner"><h3 class="qubely-info-box-title"><strong>Devops</strong></h3></div></div><div class="qubely-block-btn-wrapper"><div class="qubely-block-btn"><a class="qubely-block-btn-anchor is-medium" href="../software-development/index.html">Know More</a></div></div></div></div></div>
						</div>
						<div class="is-layout-flow wp-block-column">
						</div>
						<div class="is-layout-flow wp-block-column">
						</div>
					</div>
				</div>
				<div class="wp-block-cover" style="min-height:300px"><span aria-hidden="true" class="wp-block-cover__background has-nv-dark-bg-background-color has-background-dim-100 has-background-dim"></span>
					<div class="wp-block-cover__inner-container">
						<h2 class="has-text-align-center has-nv-site-bg-color has-text-color" style="font-size:32px">Feel Free to Connect with Rajeshwari Tech</h2>
							<%= form_tag save_enquiry_path, class: "wpcf7-form init" do %>
								<div id="responsive-form" class="clearfix">
									<div class="form-row">
										<div class="column-onethird">
											<p>Name* 
												<span class="wpcf7-form-control-wrap" data-name="leadsquared-FirstName">
													<%= text_field_tag :first_name, nil ,class: "form-control", placeholder: "First name" %>
												</span>
											</p>
										</div>
										<div class="column-onethird">

											<p>Phone 
												<span class="wpcf7-form-control-wrap" data-name="leadsquared-Mobile">
													<%= text_field_tag :phone, nil ,class: "form-control", placeholder: "Phone" %>
												</span>
											</p>
										</div>
										<div class="column-onethird">
											<p>Work Email* 
												<span class="wpcf7-form-control-wrap" data-name="leadsquared-EmailAddress">
													<%= text_field_tag :email, nil ,class: "form-control", placeholder: "Phone" %>
												</span>
											</p>
										</div>
									</div>
									<div class="form-row">
										<div class="column-full">
											<p><span class="wpcf7-form-control-wrap" data-name="your-consent"><span class="wpcf7-form-control wpcf7-acceptance"><span class="wpcf7-list-item"><label><input type="checkbox" name="your-consent" value="1" aria-invalid="false"><span class="wpcf7-list-item-label"><span style="font-size:15px;">By providing your contact details, you agree to our <a class="text_link" href="https://www.devlabsalliance.com/terms-of-use-and-privacy-policy" target="_blank">Privacy Policy</a></span>.</span></label></span></span></span>
											</p>
										</div>
									</div>
									<div class="form-row">
										<div class="column-full submit-center">
											<p>
              					<%= submit_tag "Submit",class: "wpcf7-form-control has-spinner wpcf7-submit", style: "color: #fff;border-radius: 10px;" %>
											</p>
										</div>
									</div>
								</div>
							<% end %>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</main>

<style type="text/css">
	#responsive-form{
		max-width: 100%;
